# Google Apps Script Configuration
VITE_API_BASE_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Netlify Functions (used in production)
# The frontend will proxy API calls through /.netlify/functions
VITE_NETLIFY_FUNCTIONS_URL=/.netlify/functions

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# App Configuration
VITE_APP_TITLE=LeadFlow India
VITE_APP_VERSION=1.0.0

# Environment
NODE_ENV=development

# Netlify Deployment
# These are set automatically by Netlify
# NETLIFY=true
# CONTEXT=production|deploy-preview|branch-deploy